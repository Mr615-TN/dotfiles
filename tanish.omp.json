{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#282c34",
          "foreground": "#46d9ff",
          "leading_diamond": "\ue0c7\u2593",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "style": "diamond",
          "template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }}<#5B6268> \ue0b1</>",
          "type": "os"
        },
        {
          "background": "#282c34",
          "foreground": "#51afef",
          "style": "diamond",
          "template": " {{ if ne .Env.POSH_SESSION_DEFAULT_USER .UserName }}<#98be65>{{ .UserName }}</><#5B6268>\uf1fa</>{{ end }}<#ecbe7b>{{ .HostName }}</><#5B6268> \ue0b1</>",
          "type": "session"
        },
        {
          "background": "#282c34",
          "foreground": "#ecbe7b",
          "style": "diamond",
          "template": " \uf0e7<#5B6268> \ue0b1</>",
          "type": "root"
        },
        {
          "background": "#282c34",
          "foreground": "#bbc2cf",
          "properties": {
            "folder_icon": "<#c678dd>\ue5fe </>",
            "folder_separator_icon": "<#c678dd>\ue0bf </>",
            "home_icon": "\uf015",
            "style": "agnoster_short"
          },
          "style": "diamond",
          "template": "<#ecbe7b> \ue5ff </>{{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#282c34",
          "foreground": "#ecbe7b",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ecbe7b{{ end }}",
            "{{ if gt .Ahead 0 }}#98be65{{ end }}",
            "{{ if gt .Behind 0 }}#c678dd{{ end }}"
          ],
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": "<#5B6268>\ue0b1 </>{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<#ff6c6b> \uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#98be65> \uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#282c34",
          "foreground": "#bbc2cf",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#2f2f2f",
          "foreground": "#fafafa",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "type": "text"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#6CA35E",
          "style": "diamond",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}<#7a7a7a> \ue0b3</>",
          "type": "node"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#96E072",
          "style": "diamond",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}<#7a7a7a> \ue0b3</>",
          "type": "python"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#ffcb6b",
          "style": "diamond",
          "template": " \ue7a8 {{ .Full }}<#7a7a7a> \ue0b3</>",
          "type": "rust"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#61afef",
          "style": "diamond",
          "template": " \ue627 {{ .Full }}<#7a7a7a> \ue0b3</>",
          "type": "go"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#c678dd",
          "style": "diamond",
          "template": " \ue61d {{ .Full }}<#7a7a7a> \ue0b3</>",
          "type": "cpp"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#fafafa",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }}<#007ACC> \ue641 </>",
          "trailing_diamond": "\u2593\ue0c6",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#5B6268",
          "style": "plain",
          "template": "~#@\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "final_space": true,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#5B6268",
    "template": " ~#@\u276f "
  },
  "version": 3
}

